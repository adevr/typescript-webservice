{
    "name": "typescript-webservice",
    "version": "1.0.0",
    "description": "Base for webservices built with typescript",
    "main": "dist/index.js",
    "scripts": {
        "test": "jest",
        "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
        "build": "tsc",
        "prestart": "npm run build",
        "start": "node ."
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "jest": {
        "testEnvironment": "node",
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    },
    "dependencies": {
        "@types/bcrypt": "^3.0.0",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.4",
        "@types/helmet": "0.0.45",
        "@types/jest": "^25.2.1",
        "@types/mongoose": "^5.7.10",
        "bcrypt": "^4.0.1",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "helmet": "^3.22.0",
        "jest": "^25.2.7",
        "mongoose": "^5.9.7",
        "tslint": "^6.1.1",
        "typescript": "^3.8.3"
    }
}
